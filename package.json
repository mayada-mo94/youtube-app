    "@vue/cli-plugin-unit-jest": "^4.5.12",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "msw": "^0.28.2",
    "numeral": "^2.0.6",
    "rxjs": "^6.6.7",
    "typescript": "^4.2.4",
    "vue": "^2.6.12",
    "vue-router": "^3.5.1",
    "vue-rx": "^6.2.0",
    "vue-scroll-loader": "^2.2.0",
    "vue-uuid": "^2.0.2"
  },
  "devDependencies": {
